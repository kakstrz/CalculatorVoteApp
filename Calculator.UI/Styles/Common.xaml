<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Calculator.UI.Styles">
    <Color x:Key="VeryLight">#F0F0E7</Color>
    <Color x:Key="Light">#cdd3c5</Color>
    <Color x:Key="Medium">#839b8a</Color>
    <Color x:Key="Dark">#33675f</Color>

    <SolidColorBrush Color="{StaticResource VeryLight}"  x:Key="VeryLightBrush"/>
    <SolidColorBrush Color="{StaticResource Light}" x:Key="LightBrush"/>
    <SolidColorBrush Color="{StaticResource Medium}" x:Key="MediumBrush"/>
    <SolidColorBrush Color="{StaticResource Dark}" x:Key="DarkBrush"/>
</ResourceDictionary>